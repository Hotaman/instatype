    "babel-preset-react": "6.16.0",
    "react-hot-loader": "^3.0.0-beta.0",
    "webpack": "1.13.3",
    "webpack-dev-middleware": "^1.6.1",
    "webpack-hot-middleware": "^2.10.0",
    "webpack-merge": "^1.0.2"
  },
  "dependencies": {
    "express": "^4.14.0",
    "instatype": "^1.0.12",
    "lodash": "^4.17.2",
    "react": "^15.4.1",
    "react-dom": "^15.4.1",
