{
  "name": "web",
  "version": "0.0.0",
  "description": "Simple React Typeahead",
  "dependencies": {
    "react": "~0.13.14"
  },
  "devDependencies": {
    "browserify": "latest",
    "babelify": "~6.0.2",
    "reactify": "~1.1.0",
    "watchify": "~3.1.2",
    "uglifyify": "latest",
    "precommit-hook": "latest"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/gragland/instatype.git"
  },
  "author": "",
  "license": "",
  "bugs": {
    "url": "https://github.com/gragland/typeahead/issues"
  },
  "homepage": "https://github.com/gragland/typeahead",
  "scripts": {
    "build": "browserify js-src/app.js --transform babelify --global-transform uglifyify --outfile js-build/bundle.js",
    "watch": "watchify js-src/app.js --transform babelify --outfile js-build/bundle.js -v"
  },
  "pre-commit": ["build"]
}